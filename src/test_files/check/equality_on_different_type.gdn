external fun foo(s: String, i: Int) {
    // Always False.
    if s == i {
        print("hello world")
    }
}

external fun bar(s: Option<String>, i: Option<Int>) {
    // Only True when both values are None.
    if s == i {
        print("hello world")
    }
}

