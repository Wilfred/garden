// What is the remainder when dividing `x` by `y`?
fun mod(x: Int, y: Int): Int {
  x - ((x / y) * y);
}

fun main(args: List): Void {
  let i = 1;
  while (i < 21) {
    if (mod(i, 15) == 0) {
      println("FizzBuzz");
    } else if (mod(i, 5) == 0) {
      println("Buzz");
    } else if (mod(i, 3) == 0) {
      println("Fizz");
    } else {
      println(int_to_string(i));
    }
    i = i + 1;
  }
}
