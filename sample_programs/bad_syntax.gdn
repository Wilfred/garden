
fun list_filter(items, f) {
  let result = [];

  let i = 0;
  while (i < list_length(items)) {
    let item = list_get(items, i);

    if (f(item)) {
      result = list_append(result, item);
    }

    // todo: handle parse errors better if this semicolon is missing.
    i = i + 1
  }
  result;
}

