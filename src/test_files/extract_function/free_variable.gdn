fun increment(i: Int): Int {
  i + 1
}

fun contains_expr(i: Int) {
  let x = 1

  // We want to extract x as a parameter, but not increment.
  let y = x + increment(i)
  //      ^^^^^^^^^^^^^^^^

  let _ = y + 3
}

